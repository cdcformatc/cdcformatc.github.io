function do_action(cat)
	local p = cat.p
	local b=btn(b_act,p)
	local d=btn(b_down,p)

	if (b and not d) then
		if (cat.last_act>=g_timer) return false
		-- else
		do_swipe(cat)
	end
	return true
end

